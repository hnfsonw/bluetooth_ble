<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.UpgradeFailActivity">

    <include
        android:id="@+id/include"
        layout="@layout/include_login" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/include"
        app:srcCompat="@mipmap/ic_launcher_round" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="升级失败"
        android:textSize="18sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3" />

    <Button
        android:id="@+id/upgrade_fail_btn_retry"
        android:layout_width="216dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:text="重新升级"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView17"
        android:background="@drawable/btn_shape_selector"/>

    <Button
        android:id="@+id/upgrade_fail_btn_back"
        android:layout_width="216dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/btn_shape_selector"
        android:text="返回"
        app:layout_constraintEnd_toEndOf="@+id/upgrade_fail_btn_retry"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/upgrade_fail_btn_retry"
        app:layout_constraintTop_toBottomOf="@+id/upgrade_fail_btn_retry" />

    <Button
        android:id="@+id/upgrade_fail_btn_scan_new_device"
        android:layout_width="216dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:background="@drawable/btn_shape_selector"
        android:text="扫描新设备"
        app:layout_constraintEnd_toEndOf="@+id/upgrade_fail_btn_back"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="@+id/upgrade_fail_btn_back"
        app:layout_constraintTop_toBottomOf="@+id/upgrade_fail_btn_back" />

</androidx.constraintlayout.widget.ConstraintLayout>