<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.activity.ResetPswActivity">

    <include
        android:id="@+id/include"
        layout="@layout/include_login" />


    <View
        android:id="@+id/view5_psw"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="32dp"
        android:layout_marginEnd="32dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/view2_psw" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="确认密码"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/view5_psw"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view5_psw" />

    <View
        android:id="@+id/view2_psw"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="32dp"
        android:background="#FFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        tools:layout_editor_absoluteY="270dp" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        android:text="新密码"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/view2_psw"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/view2_psw" />

    <EditText
        android:id="@+id/reset_psw_etv_pswone"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:background="@null"
        android:ems="10"
        android:hint="输入新密码"
        android:inputType="textPassword"
        android:paddingLeft="8dp"
        android:paddingTop="16dp"
        android:paddingRight="8dp"
        android:paddingBottom="16dp"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/view2_psw"
        app:layout_constraintEnd_toEndOf="@+id/view2_psw"
        app:layout_constraintStart_toEndOf="@+id/textView21" />

    <EditText
        android:id="@+id/reset_psw_etv__pswtwo"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:layout_marginStart="8dp"
        android:background="@null"
        android:ems="10"
        android:hint="再次输入密码"
        android:inputType="textPassword"
        android:paddingLeft="8dp"
        android:paddingTop="16dp"
        android:paddingRight="8dp"
        android:paddingBottom="16dp"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/view5_psw"
        app:layout_constraintEnd_toEndOf="@+id/view5_psw"
        app:layout_constraintStart_toEndOf="@+id/textView22" />


</androidx.constraintlayout.widget.ConstraintLayout>